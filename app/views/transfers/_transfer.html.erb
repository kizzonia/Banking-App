<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="header">
                <h4 class="title">Account History</h4>
                <p class="category"></p>
            </div>
            <div class="content table-responsive table-full-width">
                <table class="table table-striped">
                    <thead>
                        <th>ID</th>
                      <th>Type</th>
                      <th>Account Number</th>
                      <th>Routine</th>
                      <th>Amount</th>
                    </thead>
                    <tbody>
                      <tr>
                         <td></td>
                         <td><%= transfer.transfer_type %></td>
                         <td><%= transfer.account_number %></td>
                         <td><%= transfer.routine_number %></td>
                         <td>$<%= transfer.amount%></td>
                       </tr>
                     </tbody>
                   </table>
                 </div>
               </div>
             </div>
           </div>
