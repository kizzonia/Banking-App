}

  .sidebar{
    padding: 0 !important;


    .subtitle{
      width: 100%;
      padding: 0 !important;

      a{
        text-decoration: none;
      }
      h4{
        font-size: 15px;
        color: #333;

      }
      h2{

        font-size: 32px;
        color: #003b71;
        margin-top: 0 !important;
        padding-top: -30px !important;
        padding-bottom: 30px;
      }
    }
    .form{
      .col-md-10{
        padding-left: 5px !important;
      }
      label{
        font-family: inherit;
        font-weight: 500;
        font-size: 20px;

        color: inherit;
      }
      input{
        display: block;
        width: 100%;
        margin-left: 10px;
        height:38px;
        border-radius: 3px;
        background-clip: padding-box;
        border: 1px solid #cfcfcf;
        padding: 0 !important;

        font: 15px/15px inherit;
        color: #333;
      }
      .select {
        padding: 10px 31px 10px 10px;
        font-size: 15px;
        line-height: 17px;
        display: block;
        position: relative;
        background-color: #fff;
      }

      .select::after {
        font-family: "sun-icons-UXd1Fq6";
        display: inline-block;
        font-weight: normal;
        font-style: normal;
        text-decoration: inherit;
        content: "\e045";
        color: #ccc;
        position: absolute;
        right: 10px;
        top: 50%;
        margin-top: -12px;
        font-size: 24px;
        line-height: 24px;
        width: 11px;
      }
      select{
        width: 100%;
        height: 100%;
        font-size: 15px;
        color: #666;
        padding: 0;
        border: 0;
      }
    }
  }
