html body{
  width: 100%;
  height: 100%;
  padding:0 !important;
  margin:0 auto !important;
  overflow-x: hidden;
  font-family: 'Titillium Web', sans-serif;
  font-size: 16px;
  background-color: #f5f5f5;
  position: relative;

}


.top-menu{
  background: linear-gradient(to right,#fff 35%,#ededef 65%,#efefef 100%);
  height: 37px;
  width: 100%;
  ul{
    margin-left: 100px;
    li{
      //  margin:10px auto;
      //    box-sizing: border-box;
      a{
        text-decoration: none;
        color: #333;
        font-size: 15px;
        font-weight: 300;

      }
    }
  }
}



.navbar{
  background: #fff;

  .navbar-brand{
    margin-top: -26px;

    img{
      width:255px;
      height: 48px;
    }
  }

  a:hover{
    color: #333;
  }

}
.bottom-line{
  width: 100%;
  border-bottom: #ededef 1px solid ;


}
.bottom-nav{
  background: #fff;
  height: 40px;
  padding-top: 10px;
  padding-bottom: 10px;
  a {
    color: #333;
    font-weight: 200;
    font-size: 15px;
    padding-right: 5px;
  }
}

@media (max-width: 1199px) {
  .bottom-nav{
    display: none;
  }
}
