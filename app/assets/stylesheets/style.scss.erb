//hero banner here
.hero{
  width: 100%;
  height: 400px;
  background-position: center !important;
  background-repeat: no-repeat !important;
  background-size: cover !important;
  margin-top: 130px;
  position: relative;
  .hero-header{
    padding-top: 50px;
    padding-bottom: 40px;
    h3{
      color:#fff;
      font-weight: 600px;
      font-size: 30px;
    }

    button{
      margin-top: 40px;
    }

  }

}

