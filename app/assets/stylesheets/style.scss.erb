.classical-grid{
  width: 100%;
  height:auto;
  margin-top: 50px;

  h3{
    font-size: 30px;
    font-weight: 700;
    padding-top: 15px;
    padding-bottom: 5px;
  }
  p{
    text-align: left;
    font-size: 15px;
    font-weight: 500;
    padding-bottom: 15px;


  }

  .col-md-9{
    padding-left:0 !important;
    padding-right: 0 !important;
  }

  .col-md-5{
    padding-right:0 !important;
    padding-left: 0 !important;
  }
  .sp{
    margin: 12px;
  }
  .col-lg-6{
    margin-bottom: 20px;
  }

  .card-blue{
    background-color: #fff;
    border-top: 10px #003b71 solid !important;
    margin-top: 30px;

    .card-border{
      padding: 20px;
      p{
        text-align: left;
        font-size: 17px;
        font-weight: 500;
        padding-bottom: 15px;


      }
      a{
        color: #003b71;
        text-align: right;
        font-size: 17px;
        font-weight: 500;
        float: right;
      }
    }
  }

  .card-pink{
    background-color: #fff;
    border-top: 10px #F62459 solid !important;
    margin-top: 30px;
    .card-border{
      padding: 20px;
      p{
        text-align: left;
        font-size: 17px;
        font-weight: 500;
        padding-bottom: 15px;


      }
      a{
        color: #003b71;
        text-align: right;
        font-size: 17px;
        font-weight: 500;
        float: right;
      }
    }
  }

  .card{
    background-color: #fff;
    border-radius:5px !important;
    border: 1px #f9f9f9 solid ;
    //margin-right: 20px;
    border-style: solid;
    border-color: #dcdcdc;
    border-top-width: 1px;
    border-bottom-width: 2px;
    border-right-width: 1px;


    .card-body{
      padding: 10px;
      margin-bottom:25px;

      a{
        color: #fff;
        text-align: center;
        font-size: 17px;
        font-weight: 500;

      }

      .card-title{
        h3{
          font-size: 32px;
          line-height: 36px;
          color: rgb(0, 59, 113);
          margin-top: 5px;
          padding-right: 25px;
          font-weight: 300;
          font-family: 'Ubuntu', sans-serif;
          width: 100%;
          position: relative;
        }
      }
      p{
        //text-align: justify;
        font-weight: 200;
        font-family: "Ubuntu";


      }
    }
